<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!--通过FactoryBean来配置bean
		class：指向FactoryBean的全类名
		property：配置的是FactoryBean的属性
		注意：实际返回的实例却是FactoryBean的getObject方法返回的实例
	-->
	<bean id="car" class="com.david.spring4.helloworld.beans.factoryBean.CarFactoryBean">
		<property name="brand" value="帝豪"></property>
	</bean>
	
</beans>
